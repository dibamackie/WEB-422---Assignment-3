{
  "version": 2,
  "builds": [
    {
      "src": "my-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "my-app/$1"
    }
  ]
}
